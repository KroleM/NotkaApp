<?xml version="1.0" encoding="utf-8" ?>
<ResourceDictionary 
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:toolkit="http://schemas.microsoft.com/dotnet/2022/maui/toolkit" >

    <toolkit:IsNullConverter x:Key="isNull"/>
    <toolkit:IsNotNullConverter x:Key="isNotNull"/>
    <toolkit:InvertedBoolConverter x:Key="invertedBool"/>
    <toolkit:IsStringNotNullOrWhiteSpaceConverter x:Key="isNotNullEmptyWhiteSpace"/>
    <toolkit:IsListNotNullOrEmptyConverter x:Key="isListNotNullOrEmpty"/>

    <toolkit:ItemTappedEventArgsConverter x:Key="itemTappedEventArgsConverter" />

</ResourceDictionary>